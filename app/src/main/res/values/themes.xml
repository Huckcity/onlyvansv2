<!--    </style>-->

    <style name="customRoundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>

    <style name="Theme.OnlyVansV2.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.OnlyVansV2.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>